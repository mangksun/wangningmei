{
  "module": {
    "name": "MusicInfoCommon",
    "type": "har",
    "deviceTypes": [
      "default",
      "tablet",
      "2in1",
      "wearable"
    ],

    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET",
        "reason": "$string:Internet",
        "usedScene": {
          "abilities": [
            "EntryAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_NETWORK_INFO",
        "reason": '$string:network_info',
        "usedScene": {
          "abilities": [
            "EntryAbility"
          ],
          "when": "inuse"
        }
      },
    ]
  }
}
